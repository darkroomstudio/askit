{
  "crons": [
    {
      "path": "/api/remind/group-up",
      "schedule": "30 08 * * 0"
    },
    {
      "path": "/api/remind/stand-up",
      "schedule": "30 08 * * 3"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://vercel.com/new/clone?repository-url=https://github.com/devjiwonchoi/vercel-ai-slackbot&env=OPENAI_API_KEY,SLACK_BOT_TOKEN,SLACK_SIGNING_SECRET"
    }
  ]
}